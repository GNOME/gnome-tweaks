<Project xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:foaf="http://xmlns.com/foaf/0.1/"
         xmlns:gnome="http://api.gnome.org/doap-extensions#"
         xmlns="http://usefulinc.com/ns/doap#">

  <name xml:lang="en">GNOME Tweaks</name>
  <shortdesc xml:lang="en">Customize advanced GNOME options</shortdesc>
  <description>GNOME Tweaks allows adjusting advanced configuration
settings in GNOME. This includes things like the fonts used in user
interface elements, alternative user interface themes, changes in window
management behavior, GNOME Shell appearance, etc.</description>
  <homepage rdf:resource="https://gitlab.gnome.org/GNOME/gnome-tweaks" />
  <download-page rdf:resource="https://download.gnome.org/sources/gnome-tweaks/" />
  <bug-database rdf:resource="https://gitlab.gnome.org/GNOME/gnome-tweaks/issues" />
  <category rdf:resource="http://api.gnome.org/doap-extensions#apps" />
  <programming-language>Python</programming-language>

  <maintainer>
    <foaf:Person>
      <foaf:name>Evan Welsh</foaf:name>
      <foaf:mbox rdf:resource="mailto:contact@evanwelsh.com" />
      <gnome:userid>ewlsh</gnome:userid>
    </foaf:Person>
  </maintainer>
</Project>
